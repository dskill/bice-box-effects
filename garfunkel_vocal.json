{
  "name": "garfunkel_vocal",
  "description": "Art Garfunkel-inspired vocal doubling with cathedral reverb",
  "audio": "audio/simple_reverb.sc",
  "visual": "visual/waveform2.js",
  "params": [
    {
      "name": "doubleLevel",
      "value": 0.8,
      "range": [0.4, 1.2],
      "description": "Amount of vocal doubling effect"
    },
    {
      "name": "predelay",
      "value": 0.04,
      "range": [0.01, 0.08],
      "description": "Initial delay before reverb"
    },
    {
      "name": "roomSize",
      "value": 0.85,
      "range": [0.7, 0.95],
      "description": "Size of reverb space"
    },
    {
      "name": "wetLevel",
      "value": 0.65,
      "range": [0.4, 0.8],
      "description": "Amount of reverb effect"
    },
    {
      "name": "airAmount",
      "value": 2.0,
      "range": [0.0, 4.0],
      "description": "High frequency boost amount"
    }
  ]
}
{
  "name": "flanger",
  "description": "a classic flanger effect with adjustable parameters",
  "audio": "audio/flanger.sc",
  "visual": "visual/waveform.js",
  "params": [
    {
      "name": "rate",
      "value": 3,
      "range": [0.1, 10]
    },
    {
      "name": "depth",
      "value": 0.001,
      "range": [0, 0.02]
    },
    {
      "name": "feedback",
      "value": -0.02,
      "range": [-0.2, 0]
    },
    {
      "name": "delayTime",
      "value": 0.005,
      "range": [0.002, 0.02]
    }
  ]
}